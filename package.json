    "benchmark:smallbank-anchor": "npx tsx tests/performance/benchmarks/smallbank-anchor.ts",
    "benchmark:tpc-e": "npx tsx tests/performance/benchmarks/tpc-e-workload.ts",
    "benchmark:tpc-h": "npx tsx tests/performance/benchmarks/tpc-h-workload.ts",

    "blockbench:ycsb": "npx tsx tests/performance/benchmarks/ycsb-workload.ts",
    "blockbench:ycsb:a": "npx tsx tests/performance/benchmarks/ycsb-workload.ts a",
    "blockbench:ycsb:b": "npx tsx tests/performance/benchmarks/ycsb-workload.ts b",
    "blockbench:ycsb:c": "npx tsx tests/performance/benchmarks/ycsb-workload.ts c",

    "thesis:all": "pnpm performance:research && pnpm generate:appendix && pnpm generate:chapters && pnpm generate:abstract && pnpm generate:slides",
    "monitoring:setup": "ts-node monitoring/dashboard-setup.ts",
    "monitoring:alerts": "ts-node monitoring/alert-system.ts",
    "wallet:setup": "ts-node scripts/wallet-setup/setup-all-wallets.ts",
    "setup:loop-test": "ts-node scripts/10-setup-loop-test-standalone.ts",

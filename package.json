    "charts:generate": "npx tsx scripts/performance/generate-charts.ts",
    "export:csv": "npx tsx scripts/performance/export-csv.ts",
    "generate:appendix": "npx tsx scripts/performance/thesis-appendix.ts",
    "generate:chapters": "npx tsx scripts/performance/thesis-chapters.ts",
    "generate:abstract": "npx tsx scripts/performance/thesis-abstract.ts",
    "generate:slides": "npx tsx scripts/performance/presentation-slides.ts",

    "poa:stop": "bash poa-cluster/stop-cluster.sh",
    "poa:status": "bash poa-cluster/cluster-status.sh",
    "performance:quick-check": "ts-node scripts/performance/quick-check.ts",
    "performance:analysis": "npx tsx scripts/performance/run-analysis.ts",

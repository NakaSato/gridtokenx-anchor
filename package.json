{
  "name": "gridtokenx-anchor",
  "version": "0.1.1",
  "description": "GridTokenX Anchor Project",
  "type": "module",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "anchor test",
    "test:grx": "anchor test --skip-local-validator tests/grx-token.test.ts",
    "test:working": "ts-node test-transactions/working/run-working-test.ts",
    "test:solana": "ts-node test-transactions/solana-only/run-solana-test.ts",
    "test:performance": "ts-node tests/performance/architecture/run-performance-tests.ts",
    "test:performance-energy": "ts-node tests/performance/architecture/run-performance-tests.ts --test-suite=energy-trading",
    "test:performance-architecture": "ts-node tests/performance/architecture/run-performance-tests.ts --test-suite=architecture",
    "test:performance-benchmark": "ts-node tests/performance/architecture/run-performance-tests.ts --benchmark-only --save-json",
    "test:latency": "node --loader ts-node/esm tests/latency/run-latency-tests.ts",
    "test:latency:demo": "node tests/latency/demo-runner.js",
    "test:latency:energy-token": "node tests/latency/demo-runner.js",
    "test:latency:governance": "node tests/latency/demo-runner.js",
    "test:latency:oracle": "node tests/latency/demo-runner.js",
    "test:latency:registry": "node tests/latency/demo-runner.js",
    "test:latency:trading": "node tests/latency/demo-runner.js",
    "test:integration": "ts-node tests/integration/run-integration-tests.ts",
    "test:integration:simple": "npx tsx tests/integration/simple-integration-test.ts",
    "test:integration:direct": "npx tsx tests/integration/direct-integration-test.ts",
    "test:load": "ts-node tests/load/run-load-tests.ts",
    "test:load:high-volume": "npx mocha --timeout 600000 --require ts-node/register tests/load/high-volume-trading.test.ts",
    "test:load:concurrent": "npx mocha --timeout 600000 --require ts-node/register tests/load/concurrent-users.test.ts",
    "test:load:network": "npx mocha --timeout 600000 --require ts-node/register tests/load/network-conditions.test.ts",
    "test:load:all": "npm run test:load:high-volume && npm run test:load:concurrent && npm run test:load:network",
    "test:security": "ts-node tests/security/run-security-tests.ts",
    "test:security:authorization": "npx mocha --timeout 600000 --require ts-node/register tests/security/authorization.test.ts",
    "test:security:input-validation": "npx mocha --timeout 600000 --require ts-node/register tests/security/input-validation.test.ts",
    "test:security:replay-attacks": "npx mocha --timeout 600000 --require ts-node/register tests/security/replay-attacks.test.ts",
    "test:security:all": "npm run test:security:authorization && npm run test:security:input-validation && npm run test:security:replay-attacks",
    "test:edge-cases": "ts-node tests/edge-cases/run-edge-case-tests.ts",
    "test:edge-cases:network-failures": "node tests/edge-cases/network-failures.test.cjs",
    "test:edge-cases:data-consistency": "node tests/edge-cases/data-consistency.test.cjs",
    "test:edge-cases:boundary-values": "node tests/edge-cases/boundary-values.test.cjs",
    "test:edge-cases:all": "npm run test:edge-cases:network-failures && npm run test:edge-cases:data-consistency && npm run test:edge-cases:boundary-values",
    "test:resource": "node tests/resource/run-resource-tests.cjs",
    "test:resource:memory-leaks": "ts-node tests/resource/memory-leak-detection.test.ts",
    "test:resource:cu-optimization": "ts-node tests/resource/compute-unit-optimization.test.ts",
    "test:resource:all": "npm run test:resource:memory-leaks && npm run test:resource:cu-optimization",
    "test:all-comprehensive": "npm run test && npm run test:integration && npm run test:load && npm run test:security && npm run test:edge-cases && npm run test:resource",
    "performance:quick-check": "ts-node scripts/performance/quick-check.ts",
    "monitoring:setup": "ts-node monitoring/dashboard-setup.ts",
    "monitoring:alerts": "ts-node monitoring/alert-system.ts",
    "wallet:setup": "ts-node scripts/wallet-setup/setup-all-wallets.ts",
    "setup:loop-test": "ts-node scripts/10-setup-loop-test-standalone.ts",
    "clean": "./scripts/clean.sh"
  },
  "dependencies": {
    "@codama/nodes-from-anchor": "^1.3.0",
    "@coral-xyz/anchor": "^0.32.1",
    "@solana/kit": "^5.0.0",
    "@solana/spl-token": "^0.4.14",
    "@solana/web3.js": "^1.98.4",
    "bn.js": "^5.2.2",
    "codama": "^1.4.0"
  },
  "devDependencies": {
    "@types/bn.js": "^5.2.0",
    "@types/chai": "^4.3.20",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.10.1",
    "chai": "^4.5.0",
    "mocha": "^10.8.2",
    "ts-mocha": "^10.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "packageManager": "pnpm@10.23.0+sha512.21c4e5698002ade97e4efe8b8b4a89a8de3c85a37919f957e7a0f30f38fbc5bbdd05980ffe29179b2fb6e6e691242e098d945d1601772cad0fef5fb6411e2a4b"
}
